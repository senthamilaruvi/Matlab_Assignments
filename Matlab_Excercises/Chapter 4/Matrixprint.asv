b = input('Enter the second row vector:');
[r c] = size(a);
[d e] = size(b);
f = abs(c-e);
g = zeros(1,f);
if c > e
    mat(1,:) = a;
    mat(2,:) = [b g];
    disp(mat);
elseif c == e
    mat(1,:) = a;
    mat(2,:) = b;
